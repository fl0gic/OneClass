<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<StackPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.caden2k3.oneclass.controller.setup.CreateAccountController">
   <children>
      <ImageView fitHeight="40.0" fitWidth="40.0" StackPane.alignment="TOP_CENTER">
         <image>
            <Image url="@../image/icon.png" />
         </image>
         <StackPane.margin>
            <Insets right="150.0" top="30.0" />
         </StackPane.margin>
      </ImageView>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="title" text="OneClass" textAlignment="CENTER" StackPane.alignment="TOP_CENTER">
         <font>
            <Font size="31.0" />
         </font>
         <StackPane.margin>
            <Insets left="70.0" top="30.0" />
         </StackPane.margin>
      </Text>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="field-title" text="USERNAME">
         <StackPane.margin>
            <Insets bottom="150.0" />
         </StackPane.margin>
      </Text>
      <JFXTextField fx:id="usernameField" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets bottom="80.0" />
         </StackPane.margin>
      </JFXTextField>
      <Text strokeType="OUTSIDE" strokeWidth="0.0" styleClass="field-title" text="PASSWORD" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets top="50.0" />
         </StackPane.margin>
      </Text>
      <JFXPasswordField fx:id="passwordField" prefHeight="27.0" prefWidth="100.0" style="-fx-max-width: 200;" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets top="110.0" />
         </StackPane.margin>
      </JFXPasswordField>
      <JFXButton fx:id="createAccountButton" onAction="#handleClick" text="CREATE ACCOUNT" StackPane.alignment="CENTER">
         <StackPane.margin>
            <Insets top="250.0" />
         </StackPane.margin>
      </JFXButton>
   </children>
   <stylesheets>
      <URL value="@../styles/FieldStyle.css" />
      <URL value="@../styles/TextStyle.css" />
   </stylesheets>
</StackPane>
